{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://vk.com https://*.vk.com https://*.vkontakte.ru https://*.vk.ru"
        }
      ]
    }
  ]
}